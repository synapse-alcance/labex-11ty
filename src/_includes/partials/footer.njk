    </div>
    <div class="footer-contact-info">
      {% for info in footer.info %}
      <div class="footer-contact-single">
        <div class="footer-contact-icon">
          <img src="{{ baseurl }}{{ info.imgSrc }}" alt="{{ info.imgAlt }}">
        </div>
        <div class="footer-contact-link">
          <a href="{{ info.url }}">{{ info.text | safe }}</a>
        </div>
      </div>
      {% endfor %}
    </div>
  </div>
